@{
    ViewBag.Title = "导航";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section header{
    <link href="/Content/jquery.treeview.css" rel="stylesheet" />
    <script src="/Scripts/jquery.treeview.js" type="text/javascript"></script>
    <style type="text/css">
        #menu ul li{ font-size:14px; font-weight:bold;}
    </style>
}
@section footer{
    <script type="text/javascript">
        $(document).ready(function () {
            $("#menu>ul").treeview({
                animated: true,
                persist: "location",
                collapsed: false,
                unique: false
            });
        });
    </script>
}
<div id="menu" class="treeview-black treeview">
    @{Html.RenderAction("TreeViewRender", "Frame");}
</div>
